    "def generate_bigrams(text):\n",
    "    word=text.split()\n",
    "    bigrams=[(word[i],word[i-1]) for i in range(len(word)-1)]\n",
    "    return bigrams\n",
    "text=\"This is a sample text for generating bigrams in python\"\n",
    "result=generate_bigrams(text)\n",
    "print(result)"
