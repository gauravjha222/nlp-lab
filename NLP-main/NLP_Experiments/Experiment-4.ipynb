    "def generate_trigrams(text):\n",
    "    word=text.split()\n",
    "    bigrams=[(word[i],word[i+1],word[i+2]) for i in range(len(word)-2)]\n",
    "    return bigrams\n",
    "text=\"This is a sample text for generating bigrams in python\"\n",
    "result=generate_trigrams(text)\n",
    "print(result)"
